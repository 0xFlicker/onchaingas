{
  "name": "@0xflick/www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:start": "concurrently \"yarn dev:ssl\" \"yarn dev | bunyan\"",
    "dev": "next dev",
    "build:0xflick": "node build.mjs 0xflick.com",
    "build:nameflick": "node build.mjs nameflick.com",
    "dev:ssl": "local-ssl-proxy --source 9000 --target 3000",
    "start": "next start",
    "lint": "next lint",
    "generate": "yarn generate:schema && yarn generate:introspection && yarn generate:types",
    "generate:schema": "yarn --cwd ../../packages/graphql schema:build",
    "generate:introspection": "node scripts/possibleTypes.cjs",
    "generate:types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.94",
    "@mui/material": "^5.8.4",
    "@react-hook/resize-observer": "^1.2.5",
    "@reduxjs/toolkit": "^1.8.2",
    "classnames": "^2.3.1",
    "ethers": "^5.7.1",
    "formik": "^2.2.9",
    "formik-mui": "^5.0.0-alpha.0",
    "next": "^12.2.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.2",
    "react-spring": "^9.5.5",
    "three": "^0.141.0",
    "use-local-storage": "^2.3.6",
    "wagmi": "^0.6.6"
  },
  "devDependencies": {
    "@sls-next/lambda-at-edge": "3.8.0-alpha.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/three": "^0.141.0",
    "bunyan": "^1.8.15",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.3",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "next-transpile-modules": "^9.0.0",
    "typechain": "^8.1.0",
    "typescript": "4.7.3"
  }
}
